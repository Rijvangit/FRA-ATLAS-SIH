{
  "fields": [
    {
      "name": "name",
      "pattern": "([A-Z][a-z]+\\s[A-Z][a-z]+)"
    },
    {
      "name": "date",
      "pattern": "(\\b\\d{1,2}[\\-/]\\d{1,2}[\\-/]\\d{2,4}\\b)"
    },
    {
      "name": "id_number",
      "pattern": "([A-Z0-9\\-]{6,})"
    },
    {
      "name": "amount",
      "pattern": "(Rs\\.?\\s?\\d+[\\,\\d]*)"
    }
  ]
}