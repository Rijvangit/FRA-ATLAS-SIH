{
  "projects": {
    "fra-dashboard": {
      "root": "fra-dashboard",
      "buildCommand": "npm run build",
      "outputDirectory": "build"
    },
    "fra-map": {
      "root": "TINA 2/fra-map",
      "buildCommand": "npm run build",
      "outputDirectory": "dist"
    },
    "fra-backend": {
      "root": "fra-backend-express-complete",
      "buildCommand": "npm install && npm run build",
      "outputDirectory": "."
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/fra-backend-express-complete/$1" },
    { "source": "/map/(.*)", "destination": "/TINA 2/fra-map/dist/index.html" },
    { "source": "/(.*)", "destination": "/fra-dashboard/build/index.html" }
  ]
}

